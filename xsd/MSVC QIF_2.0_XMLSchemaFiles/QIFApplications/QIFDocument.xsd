<!-- start key and one keyref for QIF ids of feature actuals -->

    <xs:key name="FeatureActualIdKey">
      <xs:annotation>
        <xs:documentation>
          The FeatureActualIdKey collects all QIF ids of all
          FeatureActuals.
        </xs:documentation>
      </xs:annotation>
      <xs:selector
        xpath="t:MeasurementsResults/t:MeasurementResults/t:MeasuredFeatures/t:FeatureActuals/t:*"/>
      <xs:field xpath="@id"/>
    </xs:key>

    <xs:keyref name="FeatureActualIdKeyref"
      refer="FeatureActualIdKey">
      <xs:annotation>
        <xs:documentation>
          The FeatureActualIdKeyref requires that an id in the ActualIds of
          an AverageFeature be the id of a FeatureActual. This is a weak
          check since it does not require that the referenced features be
          the same type of feature as the FeatureActual of the
          AverageFeature. That check cannot be done in XSDL.
        </xs:documentation>
      </xs:annotation>
      <xs:selector
        xpath="t:Statistics/StatisticalStudiesResults/*/t:AverageFeature/t:ActualIds/t:Ids/t:Id"/>
      <xs:field xpath="."/>
    </xs:keyref>

<!-- start key and one keyref for QIF ids of CharacteristicItems -->

    <xs:key name="CharacteristicItemIdKey">
      <xs:annotation>
        <xs:documentation>
          The CharacteristicItemIdKey collects all QIF ids of all
          CharacteristicItemss.
        </xs:documentation>
      </xs:annotation>
      <xs:selector xpath="t:Characteristics/t:CharacteristicItems/t:*"/>
      <xs:field xpath="@id"/>
    </xs:key>

    <xs:keyref name="CharacteristicItemIdKeyref"
      refer="CharacteristicItemIdKey">
      <xs:annotation>
        <xs:documentation>
          The CharacteristicItemIdKeyref requires that a
          characteristic item Id located anywhere be the id of a
          CharacteristicItem.
        </xs:documentation>
      </xs:annotation>
      <xs:selector
        xpath="t:Characteristics/t:CharacteristicGroups/t:*/t:CharacteristicItemIds/t:Id
          | t:MeasurementsResults/t:MeasurementResults/t:MeasuredCharacteristics/t:CharacteristicActuals/t:*/t:CharacteristicItemId
          | t:Statistics/t:StatisticalStudyPlans/t:*/t:CharacteristicItemIds/t:Id
          | t:MeasurementPlan/t:*/t:*/t:*/t:*/t:CharacteristicItemIds/t:Id"/>
      <xs:field xpath="."/>
    </xs:keyref>

<!-- The following keys are for segmenting QIF ids by type -->

    <xs:key name="CharacteristicDefinitionIdKey">
      <xs:annotation>
        <xs:documentation>
          The CharacteristicDefinitionIdKey collects all QIF ids of all

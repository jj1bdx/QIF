        xpath="t:Features/t:FeatureNominals/t:*/t:FeatureNominalIds/t:Id
             | t:DatumTargetDefinitions/t:*/t:FeatureNominalId
             | t:Characteristics/t:CharacteristicNominals/t:*/t:FeatureNominalIds/t:Id
             | t:DatumDefinitions/t:DatumDefinition/t:FeatureNominalIds/t:Id
             | t:Product/t:PartSet/t:Part/t:FeatureNominalIds/t:Id
             | t:Product/t:AssemblySet/t:Assembly/t:FeatureNominalIds/t:Id"/>
      <xs:field xpath="."/>
    </xs:keyref>

<!-- start key for QIF ids of feature definitions -->

    <xs:key name="FeatureDefinitionIdKey">
      <xs:annotation>
        <xs:documentation>
          The FeatureDefinitionIdKey collects all QIF ids of all

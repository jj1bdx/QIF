    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="FeatureActualBaseType">
        <xs:sequence>
          <xs:element name="Axis"
            type="ActualAxisType"
            minOccurs="0">
            <xs:annotation>
              <xs:documentation>

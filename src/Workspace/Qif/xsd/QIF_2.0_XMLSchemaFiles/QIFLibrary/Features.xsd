<!-- START FEATURE ASPECT LIST TYPES -->

  <xs:complexType name="FeatureAspectsListsType">
    <xs:annotation>
      <xs:documentation>
        The FeaturesAspectsListsType defines lists of the four aspects of a
        set of inspection features.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="FeatureDefinitions"
        type="FeatureDefinitionsType"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The optional FeatureDefinitions element is a list of
            definitions of the inspection features.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FeatureNominals"
        type="FeatureNominalsType"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The optional FeatureNominals element is a list of Nominal
            inspection features.
          </xs:documentation>
        </xs:annotation>

<!-- START 1 KEY AND 3 KEYREFS FOR NOMINAL FEATURES -->

        <xs:key name="FeatureNominalKey">
          <xs:annotation>
            <xs:documentation>
              The FeatureNominalKey collects all QIF ids of FeatureNominals
              and requires that they be unique.
            </xs:documentation>
          </xs:annotation>
          <xs:selector xpath="./t:*"/>
          <xs:field xpath="@id"/>
        </xs:key>

        <xs:keyref name="GenericFeatureNominalToReferenceFeatureKeyref"
          refer="FeatureNominalKey">
          <xs:annotation>
            <xs:documentation>
              The GenericFeatureNominalToReferenceFeatureKeyref requires
              that the ReferenceFeatureNominalId of a GenericFeatureNominal
              be the QIF id of a FeatureNominal.
            </xs:documentation>
          </xs:annotation>
          <xs:selector xpath="t:GenericFeatureNominal"/>
          <xs:field xpath="t:ReferenceFeatureNominalId"/>
        </xs:keyref>

        <xs:keyref name="ReferenceFeatureNominalKeyref"
          refer="FeatureNominalKey">
          <xs:annotation>
            <xs:documentation>
              The ReferenceFeatureNominalKeyref requires that the
              ReferenceFeatureNominalId of a
              SurfaceOfRevolutionFeatureNominal be the QIF id of a
              FeatureNominal.
            </xs:documentation>
          </xs:annotation>
          <xs:selector xpath="t:SurfaceOfRevolutionFeatureNominal"/>
          <xs:field xpath="t:ReferenceFeatureNominalId"/>
        </xs:keyref>

        <xs:keyref name="CrossSectionReferenceFeatureKeyref"
          refer="FeatureNominalKey">
          <xs:annotation>
            <xs:documentation>
              The CrossSectionReferenceFeatureKeyref requires that the
              CrossSectionReferenceFeatureId of an
              ExtrudedCrossSectionFeatureNominal be the QIF id of a
              FeatureNominal.
            </xs:documentation>
          </xs:annotation>
          <xs:selector xpath="t:ExtrudedCrossSectionFeatureNominal"/>
          <xs:field xpath="t:CrossSectionReferenceFeatureId"/>
        </xs:keyref>
      </xs:element>
      <xs:element name="FeatureItems"
        type="FeatureItemsType"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The optional FeatureItems element is a list of instances of
            inspection features.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="MeasuredFeaturesType">
    <xs:sequence>
      <xs:element name="FeatureActuals"
        type="FeatureActualsType">
        <xs:annotation>
          <xs:documentation>
            The FeatureActuals element is a list of actual
            inspection features.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="MeasuredFeatures"
    type="MeasuredFeaturesType"/>

  <xs:complexType name="FeatureDefinitionsType">
    <xs:annotation>
      <xs:documentation>
        The FeatureDefinitionsType defines a list of definitions of
        inspection features.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="FeatureDefinition"
        maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Each FeatureDefinition element gives information about the
            definition of an inspection feature that may be used by one or
            more features.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="FeatureNominalsType">
    <xs:annotation>
      <xs:documentation>
        The FeatureNominalsType defines a list of nominal definitions of
        inspection features.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="FeatureNominal"
        maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Each FeatureNominal element gives information about the nominal
            definition of a unique inspection feature.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="FeatureItemsType">
    <xs:annotation>
      <xs:documentation>
        The FeatureItemsType defines a list of feature items.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="FeatureItem"
        maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Each FeatureItem element gives information about a feature
            item.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="FeatureActualsType">
    <xs:annotation>
      <xs:documentation>
        The FeatureActualsType defines a list of feature actuals.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="FeatureActual"
        maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Each FeatureActual element gives information about a feature
            actual.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

<!-- END FEATURE ASPECT LIST TYPES -->


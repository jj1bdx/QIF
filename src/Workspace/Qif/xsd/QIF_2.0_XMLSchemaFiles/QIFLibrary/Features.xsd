    <xs:choice
      minOccurs="0">
      <xs:annotation>
        <xs:documentation>
          This optional compositor selects the construction method for the
          point-defined surface.
        </xs:documentation>
      </xs:annotation>
      <xs:element name="BestFit"
        type="PointDefinedSurfaceBestFitType">
        <xs:annotation>
          <xs:documentation>

<!--  BEGIN CHARACTERISTIC STATS LISTS TYPES -->

  <xs:complexType name="CharacteristicsStatsType">
    <xs:annotation>
      <xs:documentation>
        The CharacteristicsStatsType defines a list of characteristic
        statistical evaluations.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="CharacteristicStats"
        maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Each CharacteristicStats element gives information about a
            characteristic statistical evaluation.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="CharacteristicsStats"
    type="CharacteristicsStatsType">
    <xs:annotation>
      <xs:documentation>
        The global CharacteristicsStats element represents a list of
        characteristic statistical evaluations.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

<!--  BEGIN AVERAGE FEATURE TYPE -->

  <xs:complexType name="AverageFeatureType">
    <xs:annotation>
      <xs:documentation>
        The AverageFeatureType defines a feature actual which is the
        average representation of a list of feature actuals.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="FeatureActual">
        <xs:annotation>
          <xs:documentation>
            The FeatureActual element is the average feature.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ActualIds"
        type="StatsArrayIdType">
        <xs:annotation>
          <xs:documentation>
            The ActualIds element contains a list of ids of the feature
            actuals used to calculate the average feature.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="AverageFeature"
    type="AverageFeatureType">
    <xs:annotation>
      <xs:documentation>
        The global AverageFeature element represents a single average
        feature.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="AverageFeaturesType">
    <xs:annotation>
      <xs:documentation>
        The AverageFeaturesType defines a list of average features.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="AverageFeature"
        maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Each AverageFeature element gives information about an
            average feature.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>


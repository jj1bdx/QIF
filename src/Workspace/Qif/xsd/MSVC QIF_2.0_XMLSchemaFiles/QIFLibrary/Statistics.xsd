    type="AverageFeaturesType">
    <xs:annotation>
      <xs:documentation>
        The global AverageFeatures element is a list of an average
        features.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

<!--  BEGIN SUMMARY STATISTICS TYPE -->

  <xs:complexType name="SummaryStatisticsType">
    <xs:annotation>
      <xs:documentation>
        The SummaryStatisticsType defines a summary of a single unitless
        statistical value and is the base class for summary values with
        units.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="SummaryType"
        type="StatsValuesEnumType">
        <xs:annotation>
          <xs:documentation>
            The SummaryType element is the type of statistical value being
            summarized.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Average"
        type="ActualDecimalType"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The optional Average element is the average of the statistical
            value corresponding to the "AVG" StatsValuesEnumType
            enumeration.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Maximum"
        type="ActualDecimalType"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The optional Maximum element is the maximum of the statistical
            value corresponding to the "MAX" StatsValuesEnumType
            enumeration.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Minimum"
        type="ActualDecimalType"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The optional Minimum element is the Minimum of the statistical
            value corresponding to the "MIN" StatsValuesEnumType
            enumeration.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Range"
        type="ActualDecimalType"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The optional Range element is the range of the statistical
            value corresponding to the "RANGE" StatsValuesEnumType
            enumeration.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="StandardDeviation"
        type="ActualDecimalType"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The optional StandardDeviation element is the standard
            deviation of the statistical value corresponding to the
            "STDDEV" StatsValuesEnumType enumeration.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

